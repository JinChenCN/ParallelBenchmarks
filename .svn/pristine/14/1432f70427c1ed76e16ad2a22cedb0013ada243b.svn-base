    private void executeWorkload() throws ParserConfigurationException, IOException, SAXException {//####[161]####
        ParaTask.setScheduling(scheduleType);//####[163]####
        ParaTask.setThreadPoolSize(ParaTask.ThreadPoolType.ONEOFF, THREADSNUM);//####[164]####
        TaskIDGroup g;//####[166]####
        g = new TaskIDGroup(XSD_NUMBER);//####[167]####
        for (int i = 0; i < XSD_NUMBER; i++) //####[169]####
        {//####[169]####
            Context.getOut().println("Validating " + instanceNames[i]);//####[170]####
            TaskID id = doValidationTests(loops[i], allInstanceBytes[i], schemaBoundValidator[i]);//####[173]####
            g.add(id);//####[174]####
        }//####[175]####
        try {//####[176]####
            g.waitTillFinished();//####[177]####
        } catch (Exception e) {//####[178]####
            e.printStackTrace();//####[179]####
        }//####[180]####
    }//####[181]####

                <fo:table-row height="7.827586206896552mm"><fo:table-cell border="1pt solid black" display-align="center"><fo:block margin-left="1mm">Short-term accounts payable to budget</fo:block></fo:table-cell><fo:table-cell border="1pt solid black" display-align="center"><fo:block text-align="center">170</fo:block></fo:table-cell><fo:table-cell border="1pt solid black" display-align="center"><fo:block text-align="right"/></fo:table-cell><fo:table-cell border="1pt solid black" display-align="center"><fo:block text-align="right" margin-right="1mm">1180</fo:block></fo:table-cell></fo:table-row>
